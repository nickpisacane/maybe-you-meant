{
  "name": "maybe-you-meant",
  "version": "0.1.1",
  "description": "Find deceptive prop-type typeo's in your react apps",
  "main": "lib",
  "repository": "https://github.com/nickpisacane/maybe-you-meant",
  "author": "Nick Pisacane <pisacanen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test _mocha test/.setup.js test/**/*.test.js",
    "build-lib": "babel src --out-dir lib",
    "prepublish": "npm run build-lib"
  },
  "keywords": [
    "proptypes validation",
    "proptypes typo checker",
    "prop-types validation",
    "prop-types typo checker",
    "react proptypes validation",
    "react proptypes typo",
    "react prop-types validation",
    "react prop-types typo checker",
    "react proptypes",
    "react prop-types"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "sinon": "^2.1.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "arrify": "^1.0.1",
    "lodash.some": "^4.6.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": ["test/**/*.js"]
  }
}
